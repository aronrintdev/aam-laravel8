{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nysos3/swagger-generator"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "infyomlabs/adminlte-templates": "6.0.x-dev",
        "infyomlabs/laravel-generator": "6.0.x-dev",
        "infyomlabs/swagger-generator": "dev-master",
        "laravel/framework": "6.x-dev",
        "laravel/passport": "^7.2",
        "laravel/tinker": "^2.0",
        "laravelcollective/html": "^6.0",
        "laravolt/avatar": "^3.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/fractal": "^0.18.0",
        "neomerx/json-api": "^4.0",
        "sentry/sentry-laravel": "^1.2.0",
        "tymon/jwt-auth": "^1.0.0",
        "yajra/laravel-datatables-oracle": "~9.0",
        "therobfonz/laravel-mandrill-driver": "^1.0.1"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "darkaonline/l5-swagger": "^6.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
